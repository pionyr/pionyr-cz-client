{
    "name": "pionyr/pionyr-cz-client",
    "description": "PHP API Client for API provided by Pionyr.cz",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ond≈ôej Machulda",
            "email": "ondrej.machulda@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pionyr\\PionyrCz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pionyr\\PionyrCz\\": "tests/unit/",
            "Pionyr\\PionyrCz\\IntegrationTests\\": "tests/integration/"
        }
    },
    "require": {},
    "require-dev": {
        "lmc/coding-standard": "^1.0",
        "php-coveralls/php-coveralls": "^2.0",
        "php-mock/php-mock-phpunit": "^2.1",
        "phpstan/phpstan-phpunit": "^0.9.4",
        "phpstan/phpstan-shim": "^0.9.2",
        "phpunit/phpunit": "^7.1",
        "symfony/var-dumper": "^4.0"
    },
    "scripts": {
        "all": [
            "@analyze",
            "@test"
        ],
        "analyze": [
            "vendor/bin/ecs check src/ tests/ -vvv --ansi",
            "vendor/bin/phpstan.phar analyze ./src ./tests -c phpstan.neon --level 7 --ansi"
        ],
        "test": [
            "./vendor/bin/phpunit --colors=always"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
